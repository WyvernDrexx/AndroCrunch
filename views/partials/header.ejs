<!DOCTYPE html>
<html lang="en" style="position: relative;">

<head>
    <meta name="theme-color" content="#06c">
    <meta name="msapplication-navbutton-color" content="#06c">
    <meta name="apple-mobile-web-app-status-bar-style" content="#06c">
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <% if(typeof title !== "undefined"){ %>
    <title><%= title %> </title>
    <% }else{ %>
    <title>AndroCrunch</title>
    <% } %>
    <% if(typeof keywords !== "undefined"){ %>
    <meta name="keywords" content="<%= keywords %>">
    <% }else{ %>
    <meta name="keywords"
        content="Ringtone, wallpapers, wallpapers hd, android, products, free ringtones, download, hd, mp3, presets, audio">
    <% } %>
    <% if(typeof description !== "undefined"){ %>
    <meta name="description" content="<%= description %>">
    <% }else{ %>
    <meta name="description"
        content="Best place for free ringtones, wallpapers, presets download for your mobile phone and desktop">
    <% } %>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen'
        href='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css'>
    <link rel='stylesheet' type='text/css' media='screen' href='/css/main.css'>
    <link rel="stylesheet" type="text/css" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css"
        integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400i,700|Raleway&display=swap" rel="stylesheet">
    <% if(typeof canonicalUrl !== "undefined"){ %>
    <link rel="canonical" href="<%= canonicalUrl %>" />
    <% }%>
    <meta property="og:locale" content="en_US" />
    <meta property="og:site_name" content="AndroCrunch - Best place for HD wallpapers , ringtones and android apps" />
    <% if( typeof ogTitle !== "undefined"){ %>
    <meta property="og:title" content="<%= ogTitle %>" />
    <% } %>
    <% if( typeof ogDescription !== "undefined"){ %>
    <meta property="og:description" content="<%= ogDescription %>" />
    <% } %>
    <% if(typeof canonicalUrl !== "undefined"){ %>
    <meta property="og:url" content="<%= canonicalUrl %>>" />
    <% } %>
    <% if( typeof ogImage !== "undefined"){ %>
    <meta property="og:image" content="<%= ogImage %>" />
    <% } %>
    <% if( typeof ogType !== "undefined"){ %>
    <meta property="og:type" content="<%= ogType %>" />
    <% }else{%>
    <meta property="og:type" content="website" />
    <% } %>

    <% if(typeof post !== "undefined"){ %>
    <meta property='article:author' content='https://www.youtube.com/c/AndroCrunch' />
    <meta property="article:published_time" content="<%= moment(post.created).utc().format('YYYY-MM-DD') %>" />
    <% post.tags.split(" ").forEach((tag) => { %>
    <meta property="article:tag" content="<%= tag.toLowerCase() %>" />
    <% }); %>

    <script type="application/ld+json">
        {
            "@context": "http://schema.org",
            "@type": "Article",
            "dateModified": "<%= moment(post.created).utc().format('YYYY-MM-DD') %>",
            "mainEntityOfPage" : "https://www.androcrunch.in/blogs/<%= post.customUrl %>"
            "name": "<%= post.title %>",
            "datePublished": "<%= moment(post.created).utc().format('YYYY-MM-DD') %>",
            "image": "<%= post.image %>",
            "articleBody": "<%= post.content %>",
            "url": "https://www.androcrunch.in/blogs/<%= post.customUrl %>",
            "author" : "<%= post.author %>",
            "headline": "<%= post.title %>",
            "publisher": {
                "@type": "Organization",
                "name": "AndroCrunch",
                "logo": {
                    "@type": "ImageObject",
                    "name": "AndroCrunch",
                    "width": "60",
                    "height": "600",
                    "url": "https://www.androcrunch.in/imgs/logo-big.png"
                }
            }
        }
    </script>

    <% } %>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-144100001-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];

        function gtag() {
            dataLayer.push(arguments);
        }
        gtag('js', new Date());

        gtag('config', 'UA-144100001-1');
    </script>



    <!-- JSON-LD markup generated by Google Structured Data Markup Helper. -->


    <meta name="propeller" content="c9223a8436bc9e5088a0749c7bc20bb7">
    <script src="//pushazam.com/ntfc.php?p=2717619" data-cfasync="false" async></script>

</head>

<body>
    <div id="side-toggler" class="container">
        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
        <div id="mySidenav" class="sidenav">
            <a href="/">Home</a>
            <% if(loggedUser) {%>
            <a class="text-warning" href="/author/panel">Panel</a>
            <% } %>
            <a href="/blogs">Blogs</a>
            <a href="/contents">Contents</a>
            <a href="/disclaimer">Disclaimer</a>
            <a href="/contact">Contact Us</a>
            <a href="/about_us">About Us</a>
            <a href="/advertisement">Advertisement</a>
            <a href="/privacy_policy">Privacy Policy</a>
        </div>
        <!-- Use any element to open the sidenav -->
        <span class="float-right" id="sidenavs" onclick="openNav()">
            <i style="font-size: 1.3rem;" class="fas fa-bars text-white"></i>
        </span>
        <img alt="Androcrunch Logo" class="float-left navbar-icon" src="/imgs/logo.png">
    </div>
    <nav id="nav" class="navbar navbar-expand-lg fixed-top">
        <div class="container ">
            <a class="navbar-brand text-white" href="/"><img alt="Androcrunch logo for mobile" class="navbar-icon"
                    src="/imgs/logo.png"></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <img alt="Menu Bar" class="svg-icon no-border" src="/imgs/menu.svg">
            </button>
            <div class="collapse navbar-collapse " id="navbarSupportedContent">
                <ul id="nav-content" class="navbar-nav ml-auto px-7 justify-content-center font-weight-bold text-white">
                    <% if(loggedUser) {%>
                    <li class="nav-item active">
                        <a class="nav-link text-warning" href="/author/panel">Panel <span
                                class="sr-only">(current)</span></a>
                    </li>
                    <% } else { %>
                    <li class="nav-item active">
                        <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
                    </li>
                    <% } %>
                    <li class="nav-item">
                        <a class="nav-link" href="/contents">Contents</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/blogs">Blogs</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button"
                            data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            More
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" href="/about_us">About Us</a>
                            <a class="dropdown-item" href="/contact">Contact Us</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="/disclaimer">Disclaimer</a>
                            <a class="dropdown-item" href="/advertisement">Advertisement</a>
                            <a class="dropdown-item" href="/privacy_policy">Privacy Policy</a>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <% if(error && error.length > 0) { %>
    <div class="alert alert-danger mt-6" role="alert">
        <%- error %>
    </div>
    <% } %>
    <% if(success && success.length > 0) { %>
    <div class="alert alert-success mt-6" role="alert">
        <%- success %>
    </div>
    <% } %>
    <% if(warning && warning.length > 0) { %>
    <div class="alert alert-warning mt-6" role="alert">
        <%- warning %>
    </div>
    <% } %>
    <button id="myBtn" title="Go to top"><img alt="Got to top icon" class="svg-icon" src="/imgs/top.svg"></button>
    <!-- <div class="loader-div">
        <div class="loader"></div>
    </div> -->

    <div id="#anti-ads" class="anti-adblock">
        <div class="rounded">
            <p class="head">
                You Have <span class="text-danger">adblocker</span> on Please deactivate it.
            </p>
            <p>We cannot run website without funding, so if you disable your adblock you will be supporting us. Thank
                You</p>
        </div>
    </div>