                            <a class="dropdown-item" href="/privacy_policy">Privacy Policy</a>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <% if(error && error.length > 0) { %>
    <div class="alert alert-danger mt-6" role="alert">
        <%- error %>
    </div>
    <% } %>
    <% if(success && success.length > 0) { %>
    <div class="alert alert-success mt-6" role="alert">
        <%- success %>
    </div>
    <% } %>
    <% if(warning && warning.length > 0) { %>
    <div class="alert alert-warning mt-6" role="alert">
        <%- warning %>
    </div>
    <% } %>
    <button id="myBtn" title="Go to top"><img alt="Got to top icon" class="svg-icon" src="/imgs/top.svg"></button>
