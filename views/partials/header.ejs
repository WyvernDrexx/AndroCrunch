<!DOCTYPE html>
<html lang="en" style="position: relative;">

<head>
    <meta name="theme-color" content="#06c">
    <meta name="msapplication-navbutton-color" content="#06c">
    <meta name="apple-mobile-web-app-status-bar-style" content="#06c">
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <% if(typeof title !== "undefined"){ %>
    <title><%= title %> </title>
    <% }else{ %>
    <title>AndroCrunch</title>
    <% } %>
    <% if(typeof keywords !== "undefined"){ %>
    <meta name="keywords" content="<%= keywords %>">
    <% }else{ %>
    <meta name="keywords"
        content="Ringtone, wallpapers, wallpapers hd, android, products, free ringtones, download, hd, mp3, presets, audio">
    <% } %>
    <% if(typeof description !== "undefined"){ %>
    <meta name="description" content="<%= description %>">
    <% }else{ %>
    <meta name="description"
        content="Best place for free ringtones, wallpapers, presets download for your mobile phone and desktop">
    <% } %>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen'
        href='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css'>
    <link rel='stylesheet' type='text/css' media='screen' href='/css/main.css'>
    <link rel="stylesheet" type="text/css" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css"
        integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400i,700|Raleway&display=swap" rel="stylesheet">
    <% if(typeof canonicalUrl !== "undefined"){ %>
    <link rel="canonical" href="<%= canonicalUrl %>" />
    <% }%>
    <meta property="og:locale" content="en_US" />
    <meta property="og:site_name" content="AndroCrunch - Best place for HD wallpapers , ringtones and android apps" />
    <% if( typeof ogTitle !== "undefined"){ %>
    <meta property="og:title" content="<%= ogTitle %>" />
    <% } %>
    <% if( typeof ogDescription !== "undefined"){ %>
    <meta property="og:description" content="<%= ogDescription %>" />
    <% } %>
    <% if(typeof canonicalUrl !== "undefined"){ %>
    <meta property="og:url" content="<%= canonicalUrl %>" />
    <% } %>
    <% if( typeof ogImage !== "undefined"){ %>
    <meta property="og:image" content="<%= ogImage %>" />
    <% } %>
    <% if( typeof ogType !== "undefined"){ %>
    <meta property="og:type" content="<%= ogType %>" />
    <% }else{%>
    <meta property="og:type" content="website" />
    <% } %>

    <% if(typeof post !== "undefined"){ %>
    <meta property='article:author' content='https://www.youtube.com/c/AndroCrunch' />
    <meta property="article:published_time" content="<%= moment(post.created).utc().format('YYYY-MM-DD') %>" />
    <% post.tags.split(" ").forEach((tag) => { %>
    <meta property="article:tag" content="<%= tag.toLowerCase() %>" />
    <% }); %>

    <script type="application/ld+json">
        {
            "@context": "http://schema.org",
            "@type": "Article",
            "dateModified": "<%= moment(post.created).utc().format('YYYY-MM-DD') %>",
            "mainEntityOfPage": "https://www.androcrunch.in/blogs/<%= post.customUrl %>",
            "name": "<%= post.title %>",
            "datePublished": "<%= moment(post.created).utc().format('YYYY-MM-DD') %>",
            "image": "<%= post.image %>",
            "articleBody": "<%= post.content.replace(/<[^>]*>?/gm, "") %>",
            "url": "https://www.androcrunch.in/blogs/<%= post.customUrl %>",
            "author": "<%= post.author %>",
            "headline": "<%= post.title %>",
            "publisher": {
                "@type": "Organization",
                "name": "AndroCrunch",
                "logo": {
                    "@type": "ImageObject",
                    "name": "AndroCrunch",
                    "width": "60",
                    "height": "600",
                    "url": "https://www.androcrunch.in/imgs/logo-big.png"
                }
            }
        }
    </script>
    <script type="application/ld+json">
        {
            "@context": "https://schema.org",
            "@type": "BreadcrumbList",
            "itemListElement": [{
                "@type": "ListItem",
                "position": 1,
                "name": "Blogs",
                "item": "https://www.androcrunch.in/blogs"
            }, {
                "@type": "ListItem",
                "position": 2,
                "name": "<%= post.title %>",
                "item": "https://www.androcrunch.in/blogs/<%= post.customUrl %>"
            }]
        }
    </script>
    <% } %>
